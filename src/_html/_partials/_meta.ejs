<!doctype html>
<html lang="ja">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title><%= pageList[id].title %></title>
  <meta name="description" content="<%= pageList[id].discription %>">
  <meta name="viewport" id="viewport" content="width=device-width, initial-scale=1">
  <meta name="format-detection" content="telephone=no">
  <link rel="canonical" href="<%= conf.siteURL%>">

  <link rel="stylesheet" href="<%= cssPath %>main.css">
  <%_ for(let item of stylesheet) { _%>
  <link rel="stylesheet" href="<%= cssPath %><%= item %>.css">
  <%_ } _%>
  <% if (breadcrumbList != 'undefined') { %>
    <script type="application/ld+json">
    [{
      "@context": "http://schema.org",
      "@type": "BreadcrumbList",
      "itemListElement":[
        <% breadcrumbList.forEach((item, i) => { %>
          {
            "@type": "ListItem",
            "position": <%- ++i %>,
            "item":{
              "@id": "<%- conf.siteURL %><%- pageList[item].url %>",
              "name": "<%- pageList[item].title %>"
            }
          }<% if(breadcrumbList.length === ++i) { %>,<% } %>
        <% });%>
      ]
    }]
    </script>
  <% } %>
</head>
<body>